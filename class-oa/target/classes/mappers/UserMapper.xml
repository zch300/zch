<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.soft.classoa.mapper.UserMapper">
    <select id="selectByUsername" parameterType="String" resultType="top.soft.classoa.entity.User">
        select *
        from sys_user
        where username = #{value}
    </select>

</mapper>